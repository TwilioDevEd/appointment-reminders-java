<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0" xmlns="http://java.sun.com/xml/ns/persistence">
    <persistence-unit name="Appointments-Persistence" transaction-type="RESOURCE_LOCAL">
        <class>com.twilio.appointmentreminders.models.Appointment</class>
        <properties>
            <property name="eclipselink.target-database" value="PostgreSQL"/>
            <property name="javax.persistence.jdbc.url" value="change_value"/>
            <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
            <property name="javax.persistence.jdbc.user" value="change_value"/>
            <property name="javax.persistence.jdbc.password" value="change_value"/>
            <property name="eclipselink.logging.level" value="ALL"/>
        </properties>
    </persistence-unit>
</persistence>
